  "components": {
    "schemas": {
      "CalculateLTNPURequest": {
        "required": [
          "dataCotacao",
          "dataVencimento",
          "taxaAno"
        ],
        "type": "object",
        "properties": {
          "dataCotacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataVencimento": {
            "type": "string",
            "format": "date-time"
          },
          "taxaAno": {
            "maximum": 100,
            "minimum": 0,
            "type": "number",
            "format": "double"
          }
        }
      },
      "CalculateLTNTaxaRequest": {
        "required": [
          "dataCotacao",
          "dataVencimento",
          "pu"
        ],
        "type": "object",
        "properties": {
          "dataCotacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataVencimento": {
            "type": "string",
            "format": "date-time"
          },
          "pu": {
            "maximum": 10000,
            "minimum": 0,
            "type": "number",
            "format": "double"
          }
        }
      },
      "CalculatePUResponse": {
        "type": "object",
        "properties": {
          "pu": {
            "type": "number",
            "format": "double"
          },
          "taxaAno": {
            "type": "number",
            "format": "double"
          },
          "diasUteis": {
            "type": "integer",
            "format": "int32"
          },
          "diasCorridos": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "CalculateTaxaResponse": {
        "type": "object",
        "properties": {
          "taxaAno": {
            "type": "number",
            "format": "double"
          },
          "pu": {
            "type": "number",
            "format": "double"
          },
          "diasUteis": {
            "type": "integer",
            "format": "int32"
          },
          "diasCorridos": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "CreateAtivoRequest": {
        "required": [
          "codAtivo"
        ],
        "type": "object",
        "properties": {
          "codAtivo": {
            "maxLength": 50,
            "type": "string"
          },
          "tipoAtivo": {
            "maxLength": 50,
            "type": "string",
            "nullable": true
          },
          "emissorId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "alphaToolsId": {
            "maxLength": 50,
            "type": "string",
            "nullable": true
          },
          "dataVencimento": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        }
      },
      "CreateBoletaRequest": {
        "required": [
          "tipoOperacao",
          "tipoPrecificacao"
        ],
        "type": "object",
        "properties": {
          "boletaPrincipalId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "ativoId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "contraparteId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "fundoId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "ticker": {
            "maxLength": 50,
            "type": "string",
            "nullable": true
          },
          "tipoOperacao": {
            "maxLength": 1,
            "type": "string"
          },
          "volume": {
            "type": "number",
            "format": "double"
          },
          "quantidade": {
            "type": "number",
            "format": "double"
          },
          "tipoPrecificacao": {
            "type": "string"
          },
          "ntnbReferencia": {
            "maxLength": 20,
            "type": "string",
            "nullable": true
          },
          "spreadValor": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "dataFixing": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "taxaNominal": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "pu": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "alocacao": {
